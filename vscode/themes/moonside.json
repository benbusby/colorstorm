{
    "name": "Moonside",
    "tokenColors": [
        {
            "name": "Global settings",
            "settings": {
                "background": "#000000",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#ff6693"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
            "settings": {
                "foreground": "#ff99dc"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#aaef64"
            }
        },
        {
            "name": "Variable Instances",
            "scope": [
                "variable.instance",
                "variable.other.instance",
                "variable.readwrite.instance",
                "variable.other.readwrite.instance",
                "variable.other.property",
                "meta.definition.variable",
                "support.constant",
                "support.variable"
            ],
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "Identifier",
            "scope": "variable, support.class, entity.name.function",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Function call",
            "scope": "entity.name.function, support.function",
            "settings": {
                "foreground": "#aaef64"
            }
        },
        {
            "name": "Storage",
            "scope": "storage.type, storage.modifier",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Modules",
            "scope": "support.module, support.node",
            "settings": {
                "foreground": "#8de7ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Type",
            "scope": "support.type",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "Type",
            "scope": "entity.name.type, entity.other.inherited-class",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#fD35fa",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Class",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#F6F929",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Class variable",
            "scope": "variable.object.property, meta.field.declaration entity.name.function",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "Class method",
            "scope": "meta.definition.method entity.name.function",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "Function definition",
            "scope": "meta.function entity.name.function",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "Template expression",
            "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Reset embedded/template expression colors",
            "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "YAML key",
            "scope": "entity.name.tag.yaml",
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "JSON key",
            "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "JSON constant",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "CSS class",
            "scope": "entity.other.attribute-name.class",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "CSS ID",
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#ff6693"
            }
        },
        {
            "name": "CSS tag",
            "scope": "source.css entity.name.tag",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "HTML tag outer",
            "scope": "meta.tag, punctuation.definition.tag",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "HTML tag inner",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "HTML tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#aaef64"
            }
        },
        {
            "name": "Markdown heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Markdown link text",
            "scope": "text.html.markdown meta.link.inline, meta.link.reference",
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "Markdown list item",
            "scope": "text.html.markdown beginning.punctuation.definition.list",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "Markdown italic",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#8de7ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#8de7ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold italic",
            "scope": "markup.bold markup.italic, markup.italic markup.bold",
            "settings": {
                "foreground": "#8de7ff",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Markdown code block",
            "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
            "settings": {
                "foreground": "#ff6693"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": "markup.inline.raw.string.markdown",
            "settings": {
                "foreground": "#ff6693"
            }
        },
        {
            "name": "INI property name",
            "scope": "keyword.other.definition.ini",
            "settings": {
                "foreground": "#8de7ff"
            }
        },
        {
            "name": "INI section title",
            "scope": "entity.name.section.group-title.ini",
            "settings": {
                "foreground": "#5e9aff"
            }
        },
        {
            "name": "C# class",
            "scope": "source.cs meta.class.identifier storage.type",
            "settings": {
                "foreground": "#F6F929",
                "fontStyle": "underline"
            }
        },
        {
            "name": "C# class method",
            "scope": "source.cs meta.method.identifier entity.name.function",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "C# function call",
            "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
            "settings": {
                "foreground": "#aaef64"
            }
        },
        {
            "name": "C# type",
            "scope": "source.cs storage.type",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "C# return type",
            "scope": "source.cs meta.method.return-type",
            "settings": {
                "foreground": "#F6F929"
            }
        },
        {
            "name": "C# preprocessor",
            "scope": "source.cs meta.preprocessor",
            "settings": {
                "foreground": "#fD35fa"
            }
        },
        {
            "name": "C# namespace",
            "scope": "source.cs entity.name.type.namespace",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Global settings",
            "settings": {
                "background": "#000000",
                "foreground": "#FFFFFF"
            }
        }
    ],
    "colors": {
        "focusBorder": "#5e9aff",
        "foreground": "#FFFFFF",
        "button.background": "#5e9aff",
        "button.foreground": "#ffffff",
        "dropdown.background": "#000000",
        "input.background": "#000000",
        "inputOption.activeBorder": "#5e9aff",
        "list.activeSelectionBackground": "#5e9aff80",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.dropBackground": "#5e9aff80",
        "list.focusBackground": "#5e9aff80",
        "list.focusForeground": "#FFFFFF",
        "list.highlightForeground": "#5e9aff",
        "list.hoverBackground": "#FFFFFF1a",
        "list.inactiveSelectionBackground": "#FFFFFF33",
        "activityBar.background": "#000000",
        "activityBar.dropBackground": "#5e9aff80",
        "activityBarBadge.background": "#5e9aff",
        "activityBarBadge.foreground": "#ffffff",
        "badge.background": "#5e9aff",
        "badge.foreground": "#ffffff",
        "sideBar.background": "#000000",
        "sideBarSectionHeader.background": "#000000",
        "editorGroup.dropBackground": "#5e9aff80",
        "editorGroup.focusedEmptyBorder": "#5e9aff",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.border": "#00000033",
        "tab.activeBorder": "#5e9aff",
        "tab.inactiveBackground": "#000000",
        "tab.activeModifiedBorder": "#5e9aff",
        "tab.inactiveModifiedBorder": "#385c99",
        "tab.unfocusedActiveModifiedBorder": "#4b7bcc",
        "tab.unfocusedInactiveModifiedBorder": "#385c99",
        "editor.background": "#000000",
        "editor.foreground": "#FFFFFF",
        "editorLineNumber.foreground": "#fD35fadd",
        "editorLineNumber.activeForeground": "#fD35faff",
        "editor.lineHighlightBorder": "#FFFFFF1a",
        "editor.rangeHighlightBackground": "#FFFFFF0d",
        "editorWidget.background": "#000000",
        "editorHoverWidget.background": "#000000",
        "editorMarkerNavigation.background": "#000000",
        "peekView.border": "#5e9aff",
        "peekViewEditor.background": "#000000",
        "peekViewResult.background": "#000000",
        "peekViewTitle.background": "#000000",
        "panel.background": "#000000",
        "panel.border": "#FFFFFF1a",
        "panelTitle.activeBorder": "#FFFFFF80",
        "panelTitle.inactiveForeground": "#FFFFFF80",
        "statusBar.background": "#000000",
        "statusBar.debuggingBackground": "#5e9aff",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.noFolderBackground": "#000000",
        "statusBarItem.activeBackground": "#5e9aff80",
        "statusBarItem.hoverBackground": "#FFFFFF1a",
        "statusBarItem.remoteBackground": "#5e9aff",
        "statusBarItem.remoteForeground": "#ffffff",
        "titleBar.activeBackground": "#000000",
        "pickerGroup.border": "#FFFFFF1a",
        "debugToolBar.background": "#000000",
        "selection.background": "#5e9aff"
    }
}
